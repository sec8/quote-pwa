{
  "name": "quote-pwa",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/quote": {
      "target": "http://localhost:3001"
    }
  },
  "dependencies": {
    "express": "^4.16.2",
    "localforage": "^1.5.6",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1"
  },
  "scripts": {
    "server": "NODE_ENV=production node build/server.js",
    "start": "concurrently \" PORT=3000 react-scripts start\" \"PORT=3001 babel-watch server.js\"",
    "build": "react-scripts build && NODE_ENV=production babel server.js -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
